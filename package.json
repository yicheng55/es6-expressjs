{
  "name": "es6-expressjs",
  "version": "1.0.0",
  "description": "ES6 Nodejs express 4 starter",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets env,stage-0\"",
    "build": "babel src -s -D -d dist --presets env,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "toan@tabvn.com",
  "license": "ISC",
  "dependencies": {
    "async": "^3.2.4",
    "body-parser": "^1.18.2",
    "compression": "^1.5.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.4",
    "debug": "^4.3.4",
    "express": "^4.16.2",
    "log4js": "^6.7.0",
    "moment": "^2.29.4",
    "morgan": "^1.9.0",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "regenerator-runtime": "^0.13.11",
    "url": "^0.11.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^8.34.0",
    "nodemon": "^2.0.20"
  }
}
